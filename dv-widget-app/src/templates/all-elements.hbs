<!-- Dialog elements -->
<p class="text-danger fs-8 text-center">dialog header</p>
<!-- Dialog Header -->
<h1 class="dialog-header text-center bxi-ff-secondary fw-bold mb-4">Dialog Header</h1>
<!-- End Dialog Header -->

<!-- start form -->
<form id="exampleForm" class="dialog-container bxi-color-light">
  <p class="text-danger fs-8 text-center">wide input</p>

  <!-- Wide Input -->
  <div class="mb-4">
    <label for="exampleField" class="form-label mb-1">Input label</label>
    <input type="text" id="exampleField" name="exampleField" class="form-control" />
    <!-- Optional Validation -->
    <div data-skcomponent="skerrormessage" class="text-danger" data-skerrorid="exampleField"></div>
    <p class="bxi-color-primary">color marked bottom label</p>
  </div>
  <!-- End Wide Input -->

  <p class="text-danger fs-8 text-center">narrow input</p>

  <!-- Narrow Input -->  
  <div class="mb-4 row">
    <div class="col-8 offset-2 col-sm-6 offset-sm-3">
      <label for="exampleFieldNarrow" class="form-label mb-1">Input label</label>
      <input type="text" id="exampleFieldNarrow" name="exampleFieldNarrow" class="form-control" />
      <!-- Optional Validation -->
      <div data-skcomponent="skerrormessage" class="text-danger" data-skerrorid="exampleFieldNarrow"></div>
    </div>
  </div>
  <!-- End Narrow Input -->

  <p class="text-danger fs-8 text-center">text block</p>

  <!-- Centered Text -->
  <p class="mb-0 text-center">Text - no bottom space</p>
  <!-- End Centered Text -->

  <p class="text-danger fs-8 text-center">wide select</p>

  <!-- Wide Dropdown -->
  <div class="mb-3">
    <label for="exampleSelect" class="form-label mb-1">Input label</label>
    <select id="exampleSelect" name="exampleSelect" class="form-select">
      <option>Option1</option>
      <option>Option2</option>
    </select>
    <!-- Optional Validation -->
    <div data-skcomponent="skerrormessage" class="text-danger" data-skerrorid="exampleSelect"></div>
  </div>
  <!-- End Wide Dropdown -->

  <p class="text-danger fs-8 text-center">narrow select</p>

  <!-- Narrow Dropdown -->
  <div class="mb-3 row">
    <div class="col-8 offset-2 col-sm-6 offset-sm-3">
      <label for="exampleSelectNarrow" class="form-label mb-1">Input label</label>
      <select id="exampleSelectNarrow" name="exampleSelectNarrow" class="form-select">
        <option>Option1</option>
        <option>Option2</option>
      </select>
      <!-- Optional Validation -->
      <div data-skcomponent="skerrormessage" class="text-danger" data-skerrorid="exampleSelectNarrow"></div>
    </div>
  </div>
  <!-- End Narrow Dropdown -->

  <p class="text-danger fs-8 text-center">text block with bottom space</p>

  <!-- Centered Text w/ Padding -->
  <p class="text-center mb-5">
    Text line<br/>+ bottom space
  </p>
  <!-- Centered Text w/ Padding -->

  <p class="text-danger fs-8 text-center">block picker with svg icons</p>

  <!-- Picker w/ Icons -->
  <div class="py-4">
    <button class="bxi-radio mb-3" data-skcomponent="skbutton" data-skbuttontype="form-submit" data-skform="exampleForm" data-skbuttonvalue="email">
      <div class="row">
        <div class="col-10 p-4">
          <p class="mb-0">Email me a one time passcode on:</p>
          <p class="mb-0 bxi-color-dark">iel***@bxindustry.com</p>
        </div>
        <div class="col pe-4 d-flex align-items-center justify-content-end">
          <svg fill="none" height="18" viewbox="0 0 22 18" width="22" xmlns="http://www.w3.org/2000/svg">
